<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <style name="EditTextplate_Theme" parent="android:Widget.EditText">
	  <item name="android:background">@drawable/plate_theme_edit_text_holo_light</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="AutoCompleteTextViewplate_Theme" parent="android:Widget.AutoCompleteTextView">
      <item name="android:dropDownSelector">@drawable/plate_theme_list_selector_holo_light</item>
	  <item name="android:background">@drawable/plate_theme_edit_text_holo_light</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="CheckBoxplate_Theme" parent="android:Widget.CompoundButton.CheckBox">
      <item name="android:button">@drawable/plate_theme_btn_check_holo_light</item>
  </style>

  <style name="RadioButtonplate_Theme" parent="android:Widget.CompoundButton.RadioButton">
      <item name="android:button">@drawable/plate_theme_btn_radio_holo_light</item>
  </style>

  <style name="Buttonplate_Theme" parent="android:Widget.Button">
	  <item name="android:background">@drawable/plate_theme_btn_default_holo_light</item>
	  <item name="android:minHeight">48dip</item>
	  <item name="android:minWidth">64dip</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="ImageButtonplate_Theme" parent="android:Widget.ImageButton">
	  <item name="android:background">@drawable/plate_theme_btn_default_holo_light</item>
  </style>

  <style name="Spinnerplate_Theme" parent="android:Widget.Spinner">
      <item name="android:background">@drawable/plate_theme_spinner_background_holo_light</item>
      <item name="android:dropDownSelector">@drawable/plate_theme_list_selector_holo_light</item>
  </style>

  <style name="SpinnerDropDownItemplate_Theme" parent="android:Widget.DropDownItem.Spinner">
      <item name="android:checkMark">@drawable/plate_theme_btn_radio_holo_light</item>
  </style>
  <style name="Tabplate_Theme">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/plate_theme_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextplate_Theme">
	  <item name="android:textColor">#000000</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ProgressBarplate_Theme" parent="android:Widget.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/plate_theme_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/plate_theme_progress_indeterminate_horizontal_holo_light</item>
      <item name="android:minHeight">16dip</item>
      <item name="android:maxHeight">16dip</item>
  </style>

  <style name="SeekBarplate_Theme" parent="android:Widget.SeekBar">
      <item name="android:progressDrawable">@drawable/plate_theme_scrubber_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/plate_theme_scrubber_progress_horizontal_holo_light</item>
      <item name="android:minHeight">13dip</item>
      <item name="android:maxHeight">13dip</item>
      <item name="android:thumb">@drawable/plate_theme_scrubber_control_selector_holo_light</item>
      <item name="android:thumbOffset">16dip</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
  </style>

  <style name="RatingBarplate_Theme" parent="android:Widget.RatingBar">
      <item name="android:progressDrawable">@drawable/plate_theme_ratingbar_full_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/plate_theme_ratingbar_full_holo_light</item>
  </style>

  <style name="RatingBarBigplate_Theme" parent="android:Widget.RatingBar">
      <item name="android:progressDrawable">@drawable/plate_theme_ratingbar_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/plate_theme_ratingbar_holo_light</item>
      <item name="android:minHeight">35dip</item>
      <item name="android:maxHeight">35dip</item>
  </style>

  <style name="RatingBarSmallplate_Theme" parent="android:Widget.RatingBar">
      <item name="android:progressDrawable">@drawable/plate_theme_ratingbar_small_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/plate_theme_ratingbar_small_holo_light</item>
      <item name="android:minHeight">16dip</item>
      <item name="android:maxHeight">16dip</item>
  </style>

  <style name="Toggleplate_Theme" parent="android:Widget.Button.Toggle">
      <item name="android:background">@drawable/plate_theme_btn_toggle_holo_light</item>
      <item name="android:minHeight">48dip</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="ListViewplate_Theme" parent="android:Widget.ListView">
      <item name="android:listSelector">@drawable/plate_theme_list_selector_holo_light</item>
  </style>

  <style name="ListViewplate_Theme.White" parent="android:Widget.ListView.White">
      <item name="android:listSelector">@drawable/plate_theme_list_selector_holo_light</item>
  </style>

  <style name="SpinnerItemplate_Theme" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
      <item name="android:textColor">#000000</item>
  </style>

